{
  "manifest_version": 2,
  "name": "Professional Codes Reader",
  "description": "Browse the web like a professional programmer!",
  "version": "1.1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "storage",
    "tabs", "http://*/*"
  ]
}
